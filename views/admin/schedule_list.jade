extends ../layout

block header
	header.bar.bar-nav
		a.btn.btn-link.btn-nav.pull-left(href="/admin", data-transition="slide-out")
			span.icon.icon-left-nav
			| Back
		h1.title= title

block content
	br
	input#name-search(type="text", placeholder="Search for name")

	ul.table-view
		for name, index in names
			li.table-view-cell
				a.schedule-name.navigate-right(data-ignore="push", href="/admin/schedule/#{name.username}", target="_blank")= name.name

block footer
	br